{
	"$basetexture" "models/metroid_prime/corruption/pirate_trooper/advanced_armor_effect"
	"$translucent" 1
	"$additive" 1

	"$detail"		"models/metroid_prime/corruption/pirate_trooper/advanced_armor_effect"
	"$detailscale"	1
	"$detailblendfactor"		1
	"$detailblendmode"		5

	$emissiveBlendEnabled 1
	$emissiveBlendTexture "models/metroid_prime/corruption/pirate_trooper/advanced_armor_effect"
	$emissiveBlendBaseTexture "models/metroid_prime/corruption/pirate_trooper/advanced_armor_effect"
	$emissiveBlendFlowTexture "requiredtoflow"
	$emissiveBlendTint "[1.5 1.5 1.5]"
	$emissiveBlendStrength 1
